<body>
	<!-- top nav -->
	<div class="topBar fixed-top" >
		<div class="row">
			<div class="col">
                {{!-- <a role="button" class="btn logoutBtn" href="../">Logout</a> --}}
				<a href="/"><img src="images/backButton.png" class="topIcons"></a>
			</div>
			<div class="col-8">
				<h4>Home</h4>
			</div>
			<div class="col">
				<a href="help"><img src="images/infoButton.png" class="topIcons" id="infoButton"></a>
			</div>
		</div>
	</div>
	<div class="containerIndex">
		<div id="welcome">
            <br>
            <p class="p4">Welcome, <span id="name">User</span></p>
	    </div>
		<!--This is a comment <img src="images/83401061_598130577414441_7838613489009557504_o.jpeg" width="600" height="400" alt="S win image"> -->
        <div class="contentBorder" id="mypets">
            <p class="p7">My pets:</p>
            <div class="row">
                <div class="column petColumn">
                    <img src="images/popcorn.jpg" alt="logo" title="Popcorn" style="width:70%">
                    <p class="p8 petName">Popcorn</p>
                </div>
                <div class="column petColumn">
                    <img src="images/maya.jpg" alt="logo" title="Maya" style="width:70%">
                    <p class="p8 petName">Maya</p>
                </div>
            </div> 
        </div>
	    <br>
		<div class="contentBorder" id="Attending">
			<!-- This is a comment <img src="images/83401061_598130577414441_7838613489009557504_o.jpeg" width="600" height="400" alt="S win image"> -->
			<p class="p7">I'm attending:</p>
			{{#each events}}
			{{#if attending}}
	            <div class="card">
	                <div class="card-body">
	                    <div class="row card-elements">
	                        <div class="card-thumbnail">
	                            <!-- public/images/popcorn.jpg -->
	                            <img class="card-img" src="images/{{image}}"> 
	                        </div>
	                        <div class="card-description">
	                            <h5 class="card-title">{{eventName}}</h5>
	                            <h6>[{{petType}}: {{breed}}]</h6>
	                            <p class="card-text">{{eventType}} Event</p>
	                            <p class="card-text">{{time}}</p>
	                            <p class="card-text">@{{location}}</p>	
	                        </div>
	                        <div class="card-button">
	                            <a href="blankEvent/{{eventName}}" class="btn btn-go">Go!</a>
	                        </div>
	                    </div>
	                </div>
	            </div>
	        {{/if}}
            {{/each}}
            <a role="button" class="btn" id="searchEventBtn" href="search">Search for events?</a>
		</div>
